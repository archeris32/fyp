extends default
block content

    style.
        .card {
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
        max-width: 300px;
        margin: auto;
        text-align: center;
        font-family: arial;
        }

        .title {
        color: grey;
        font-size: 18px;
        }

        button {
        border: none;
        outline: 0;
        display: inline-block;
        padding: 8px;
        color: white;
        background-color: #000;
        text-align: center;
        cursor: pointer;
        width: 100%;
        font-size: 18px;
        }

        .card a {
        text-decoration: none;
        font-size: 22px;
        color: black;
        }

        button:hover, a:hover {
        opacity: 0.7;
        }
    h2(style="text-align:center") #{username}
    div(class="card" )
        img(src="data:image/jpeg;base64,")
        for d in data

            h1 Name :
            input(type="text" value= username style="border-radius:10px;")
            h3 Email
            input(type="text" value= d.email style="border-radius:10px;")
            p(class="title")
            
            div(style="margin:24px 0;")
                a(href="")
                    i(class="fa fa-dribbble")  
                a(href="")
                    i(class="a fa-twitter") 
                a(href="")
                    i(class="fa fa-linkedin")  
                a(href="")
                    i(class="fa fa-facebook")  
            button Contact