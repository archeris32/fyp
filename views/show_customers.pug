extends default

block content
    table(class="table table-hover")
        thead
            tr
                th First Name                 
                th Last Name 
                th Phone 
                th Doctor 
                th Email 
                th Cost
        tbody
            for cp,index in data
                tr
                    td= cp.firstName
                    td= cp.lastName 
                    td=cp.phone 
                    td=cp.doctor 
                    td=cp.email 
                    td=cp.cost
                    td
                        a(href=`#` class=`btn btn-primary` id=`edit-button`) Edit
                        